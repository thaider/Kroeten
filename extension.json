{
	"name": "Kroeten",
	"version": "0.0.1",
	"author": [
		"Tobias Haider"
	],
	"license-name": "GPL-3.0-or-later",
	"AutoloadClasses": {
		"KroetenHooks": "Kroeten.hooks.php"
	},
	"ResourceModules": {
		"ext.kroeten": {
			"position": "top",
			"scripts": [
				"jquery.marquee.min.js",
				"ext.kroeten.js"
			],
			"messages": [
				"kroeten-ticker"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "Kroeten/modules"
	},
	"Hooks": {
		"BeforePageDisplay": [
			"KroetenHooks::onBeforePageDisplay"
		]
	},
	"MessagesDirs": {
		"Kroeten": [
			"i18n"
		]
	},
	"manifest_version": 2
}
